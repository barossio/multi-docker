{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "client",
            "image": "baross/multi-client",
            "hostname": "client",
            "essential": false,
            "memory": 128
        },
        {
            "name": "server",
            "image": "baross/multi-server",
            "hostname": "api ",
            "essential": false,
            "memory": 128
        },
        {
            "name": "worker",
            "image": "baross/multi-worker",
            "hostname": "worker",
            "essential": false,
            "memory": 128
        },
        {
            "name": "nginx",
            "image": "baross/multi-nginx",
            "hostname": "nginx",
            "essential": true,
            "memory": 128,
            "portMappings": [
                {
                    "hostPost": 80,
                    "containerPort": 80
                }
            ],
            "links": [
                "client","server"
            ]
        }
    ]
}